{
  "manifest_version": 3,
  "name": "React Chrome Extension",
  "description": "A simple React extension to motivate",
  "version": "1.0.0",
  "icons": { "48": "assets/icon48.png" },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["alarms", "notifications"]
}
